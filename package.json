    "scripts": {
        "prebuild": "npm run-s clean && npm run-s lint && npm run-s test",
        "build": "cross-env NODE_ENV=production webpack --display-chunks",
        "build:dev": "npm run-s clean && cross-env NODE_ENV=development webpack",
        "clean": "del-cli dist",
        "lint": "npm run-s lint:js && npm run-s lint:ts",
        "lint:js": "eslint -c .eslintrc.yml *.js server",
        "lint:ts": "tslint src/**/*.ts",
        "start": "npm run-s server:dev",
        "server:dev": "cross-env NODE_ENV=development webpack-dev-server",
        "test": "cross-env NODE_ENV=test karma start --single-run",
        "test:watch": "cross-env NODE_ENV=test karma start",
        "show": "webpack-dev-server --inline --hot",
        "precommit": "npm run-s lint"
    },
